<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
<TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/tytulWszystkie"
    android:text="WSZYSTKIE PRZEPISY"/>
    <AutoCompleteTextView
    android:layout_below="@id/tytulWszystkie"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/szukajTXT"
    android:hint="Wprowadź nazwę potrawy"/>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/listaPrzepis"
        android:layout_below="@id/szukajTXT"></ListView>


</RelativeLayout>